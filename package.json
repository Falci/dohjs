{
  "name": "dohjs",
  "version": "0.1.0",
  "description": "DNS over HTTPS Javascript Client",
  "main": "lib/doh.js",
  "dependencies": {
    "argparse": "^1.0.10",
    "browserify": "^16.5.0",
    "dns-packet": "^5.2.1"
  },
  "devDependencies": {
    "http-server": "^0.12.1",
    "terser": "^4.6.3"
  },
  "scripts": {
    "test": "./test.sh",
    "start": "http-server public",
    "build": "cd public/ && browserify main.js -o bundle.js",
    "prestart": "npm run build",
    "prepare": "browserify lib/doh.js -o dist/doh.js && npx terser dist/doh.js --compress --mangle --output=dist/doh.min.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/byu-imaal/dohjs.git"
  },
  "keywords": [
    "DNS",
    "DoH",
    "DNS over HTTPS"
  ],
  "author": "",
  "license": "GPL-3.0+",
  "bugs": {
    "url": "https://github.com/byu-imaal/dohjs/issues"
  },
  "homepage": "https://github.com/byu-imaal/dohjs#readme",
  "bin": {
    "dohjs": "./bin/doh.js"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "optionalDependencies": {}
}
